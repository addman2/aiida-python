            "test.calc_numpy_without_numpy = tests.classme_numpy_without_numpy:ClassThatCannotStartWithTestAndWorksWithNumpyButIsWithoutNumpy",
            "test.calc_error = tests.classme_error:ClassThatCannotStartWithTestAndDoesError"
        ],
        "aiida.parsers": [
            "aiida_python.parser = aiida_python.parser:ParserPython"
        ],
        "aiida_python.serializers": [
            "int = aiida_python.serializers:SerializerInt"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=2.0.0",
	"pytest",
	"pgtest"
    ],
    "extras_require": {
        "testing": [
        ],
        "pre-commit": [
        ],
        "docs": [
        ]
